{
  "name": "tina-alpaca",
  "version": "0.1.0",
  "description": "NextJS documentation starter with TinaCMS",
  "keywords": [
    "nextjs",
    "tinaCMS"
  ],
  "author": "pravnyadv.github.io/unpackage",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "debug": "NODE_OPTIONS='--inspect' next",
    "build": "next build",
    "start": "cross-env NODE_ENV=production next start",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "create-indices": "npx babel-node ./indices/createIndices.js"
  },
  "dependencies": {
    "@babel/core": "7.18.13",
    "@babel/node": "^7.18.10",
    "@tinacms/api-git": "^0.39.0",
    "@tinacms/git-client": "^0.39.0",
    "@types/json-schema": "^7.0.11",
    "@types/node": "14.18.26",
    "@types/parse-json": "^4.0.0",
    "@types/q": "^1.5.5",
    "@zeit/next-sass": "^1.0.1",
    "algoliasearch": "^4.14.2",
    "amdefine": "^1.0.1",
    "ansi-red": "^0.1.1",
    "axios": "0.21.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-styled-components": "1.13.3",
    "caller-path": "^2.0.0",
    "code-point-at": "^1.1.0",
    "coffee-script": "^1.12.7",
    "cors": "2.8.5",
    "cross-env": "^7.0.3",
    "css-select": "^2.1.0",
    "css-select-base-adapter": "^0.1.1",
    "dotenv": "^8.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.31.1",
    "express": "4.18.1",
    "fast-glob": "3.2.11",
    "file-loader": "^6.2.0",
    "final-form-calculate": "^1.3.2",
    "gray-matter": "4.0.3",
    "gulp-header": "^1.8.12",
    "has-ansi": "^2.0.0",
    "husky": "^4.3.8",
    "json-parse-better-errors": "^1.0.2",
    "lint-staged": "^10.5.4",
    "markdown-toc": "^1.2.0",
    "moment": "^2.29.4",
    "nanoid": "^3.3.4",
    "next": "^10.2.3",
    "next-svgr": "^0.0.2",
    "next-tinacms-github": "^0.40.1",
    "next-tinacms-json": "^0.40.1",
    "next-tinacms-markdown": "0.40.1",
    "node-sass": "^6.0.1",
    "number-is-nan": "^1.0.1",
    "path-type": "^4.0.0",
    "prettier": "^2.0.2",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-instantsearch-dom": "^6.32.1",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^12.2.1",
    "react-textarea-autosize": "8.3.4",
    "react-tinacms-editor": "^0.40.1",
    "react-tinacms-inline": "^0.40.1",
    "remarkable": "2.0.1",
    "require-from-string": "^2.0.2",
    "set-getter": "^0.1.1",
    "slugify": "^1.6.5",
    "stable": "^0.1.8",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7",
    "tinacms": "^0.40.1",
    "to-arraybuffer": "^1.0.1",
    "to-regex-range": "^5.0.1",
    "toml": "^2.3.6",
    "typeface-source-code-pro": "^1.1.13",
    "unquote": "^1.1.1",
    "util.promisify": "^1.0.1",
    "whatwg-url": "^7.1.0",
    "yocto-queue": "^0.1.0"
  },
  "overrides": {
    "@zeit/next-sass": {
      "node-sass": "$node-sass"
    },
    "next": {
      "node-sass": "$node-sass"
    },
    "react-tinacms-github": {
      "@tinacms/fields": "^0.40.0"
    },
    "react-tinacms-inline": {
      "react-dropzone": ">=10.2.1"
    },
    "react-tinacms-editor": {
      "prosemirror-utils": "^1.0.0-0"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,json}": [
      "prettier --write"
    ]
  }
}
